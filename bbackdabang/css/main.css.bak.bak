@charset "utf-8";
body,header,article,section,footer,div,h1,h2,h3,h4,ul,li,p,hr{margin:0; padding:0;}
.hidden{display:none;}
ul,li{list-style:none;}
a{text-decoration:none;}
hr{border:none;}


/* 헤더 */
header{width:100%; height:123px; background-color:#fff; padding-top:18px; position:fixed; top:0; left:0; right:0; z-index:1;}
header div{width:1150px; height:123px; margin:0 auto; }
header div nav#nav1{width:1150px; height:123px; position:relative;}
header div#util a#mlogo{}
header div h1#logo{width:194px; height:66px; float:left; text-indent:-9999px; background-image:url(../images/logo.png); background-size:cover; margin-top:18px; margin-left:-8px;}
header div nav ul{}
header div nav#nav1 ul#ul1{position:absolute; right:0; width:233px; height:21px; font-size:12px; background:#fff; line-height:21px; margin-top:8px; right:5px;}
header div nav#nav1 ul#ul1 li{float:left;}
header div nav#nav1 ul#ul1 li a{color:gray;}
header div nav#nav1 ul#ul1 li:nth-child(1)::after{content:""; display:block; width:1px; height:10px; background:gray; float:right; margin-top:5.5px; margin-left:14px; margin-right:19px;}
header div nav#nav1 ul#ul1 li:nth-child(2)::after{content:""; display:block; width:1px; height:10px; background:gray; float:right; margin-top:5.5px; margin-left:14px; margin-right:19px;}
header div nav#nav1 ul#ul1 li:nth-child(3){width:20px; height:20px; background:url(../images/facebook-ico.png); text-indent:-9999px; background-size:cover; margin-right:17px;}
header div nav#nav1 ul#ul1 li:nth-child(4){width:20px; height:20px; background:url(../images/insta-ico.png); text-indent:-9999px; background-size:cover; }
header div nav#nav1 ul#ul2{position:absolute; top:70px; right:10px; width:828px; height:17px; background-color:#fff; line-height:17px; font-size:14px; font-weight:bold;}
header div nav#nav1 ul#ul2 li{float:left; height:17px; margin-left:82px; margin-top:-3px;}
header div nav#nav1 ul#ul2 li:first-child{margin-left:0;}
header div#util2{width:100%; height:160px; background:yellow;}
header div nav#nav2{width:1150px; height:160px; background:yellow; margin:0 auto;}
header div nav#nav2 div{width:828px; height:160px; float:right; text-align:left; font-size:14px;}
header div nav#nav2 div ul{float:left; background:url(../images/menu-arr.png) no-repeat left top; padding-top:18px;}
header div nav#nav2 div ul li:hover{font-weight:bold;}
header div nav#nav2 div ul li{margin-bottom:12px;}
header div nav#nav2 div ul:first-child{margin-left:-10px;}
header div nav#nav2 div ul:nth-child(2){margin-left:48px;}
header div nav#nav2 div ul:nth-child(3){margin-left:-7px;}
header div nav#nav2 div ul:nth-child(4){margin-left:22px;}
header div nav#nav2 div ul:nth-child(5){margin-left:69px;}
header div nav#nav2 div ul:nth-child(6){margin-left:79px;}

/* 비주얼 */
article{width:1920px; height:600px; margin:123px auto 0; background:red; background-size:cover; position:relative; top:123px;}
article > div{width:1920px; height:600px;}
article > div > ul.banner{width:100%; height:100%; overflow:hidden; white-space:nowrap; position:relative;}
article > div > ul.banner > li{width:100%; height:100%; text-indent:-9999px;  position:absolute;}
article > div > ul.banner > li:nth-child(1){background:url(../images_main/main_1.jpg) no-repeat center;}
article > div > ul.banner > li:nth-child(2){background:url(../images_main/main_2.jpg) no-repeat center; left:100%;}
article > div > ul.banner > li:nth-child(3){background:url(../images_main/main_3.jpg) no-repeat center; left:200%;}


