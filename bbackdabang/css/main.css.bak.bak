/* 푸터 */
footer{position:relative; bottom:0;}
div#footer{height:257px; border-top:3px solid #ffeb00;}
div#footer > div{width:1150px; height:254px; margin:auto;}

div#footer h3{width:179px; height:47px; margin:30px auto; text-indent:-9999px; background:url(../images/flogo.png); background-size:cover; margin-bottom:35px;}
div#footer div#address{width:516px; height:142px; margin:auto; text-align:center;}
div#footer div address{font-style:normal; line-height:22px; font-size:12px; margin-bottom:40px;}
div#footer div address span{border-right:1px solid #000; padding:0 16px;}
div#footer div address span:nth-child(3){border-right:none;}
div#footer div address span:nth-child(2)#bo{border-right:none;}
div#footer div p#copy{text-transform:uppercase; font-size:13px;}

div#footer div#family{float:right; width:175px; height:24px; border:1px solid #000; font-size:13px; line-height:24px;}
div#footer div#family a{margin-left:7px;}
div#footer h4 a{width:60px; height:60px; background:url(../images/off.png) no-repeat; position:fixed; right:200px; bottom:20px;}