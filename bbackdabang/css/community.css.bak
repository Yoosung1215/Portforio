@charset "utf-8";
body,header,article,section,footer,div,h1,h2,h3,h4,ul,li,p,hr{margin:0; padding:0;}
.hidden{display:none;}
ul,li{list-style:none;}
a{text-decoration:none; color:#000;}
hr{border:none;}

/* 헤더영역 */
header{width:100%; height:123px; background-color:#fff; padding-top:20px; position:fixed; z-index: 999; top:0;}
div#yellow{display: none; width: 100%; height: 177px; background-color: aqua;}

/* util */
header div#mein{width:1150px; height:103px; background-color:#fff; margin:0 auto;}
header div#mein h1{width:194px; height:66px; float:left; margin-top:16px; margin-left:-7px;}
header div#mein div#util{float:right; width:232px; height:20px; font-size:11px; margin-top:7px; margin-right:10px;}
header div#mein div#util ul li{float:left; line-height:20px; }
header div#mein div#util ul li a{color:gray;}
header div#mein div#util ul li:nth-child(1)::after{content:""; display:block; width:1px; height:10px; background:gray; float:right; margin-left:13px; margin-right:19px; margin-top:5px;}
header div#mein div#util ul li:nth-child(2)::after{content:""; display:block; width:1px; height:10px; background:gray; float:right; margin-left:15px; margin-right:21px; margin-top:5px;}
header div#mein div#util ul li:nth-child(3){width:20px; height:20px; background:url(../images/facebook-ico.png) no-repeat; background-size:cover; text-indent:-9999px; margin-right:22px;}
header div#mein div#util ul li:nth-child(4){width:20px; height:20px; background:url(../images/insta-ico.png) no-repeat; background-size:cover; text-indent:-9999px;}

/* nav */
header div#mein nav{width:826px; height:183px; float:right; margin-top: 38px; margin-right: 8px;}
header div#mein nav ul#first > li{float:left; font-size: 13px; font-weight: bold; }
header div#mein nav ul#first li ul{padding-top: 18px; margin-top:21px; font-weight:normal;}
header div#mein nav ul#first li ul li{margin-bottom: 8px; font-size: 14px;}
header div#mein nav ul#first li ul li:hover{font-weight: bold; font-size: 14px;}
header div#mein nav ul#first li:nth-child(1){width:124px;}
header div#mein nav ul#first li:nth-child(2){width:109px;}
header div#mein nav ul#first li:nth-child(3){width:110px;}
header div#mein nav ul#first li:nth-child(4){width:138px;}
header div#mein nav ul#first li:nth-child(5){width:137px;}
header div#mein nav ul#first li:nth-child(6){width:137px;}
header div#mein nav ul#first li:nth-child(7){width:71px;}
header div#mein nav ul#first  li ul{width:72px;}
header div#mein nav ul#first li:hover ul{background:url(../images/menu-arr.png) no-repeat;}
header div#mein nav ul#first > li > ul:nth-child(5){background:url(../images/menu-arr.png) no-repeat center top;}
div#headerfoot{width: 100%; height:157px; background: url(../images/menu-bg.jpg);}

/*article*/
article{height:500px; margin-top:123px; margin-bottom:90px; background:url(../images_community/bgclass.jpg) no-repeat center; background-size:cover;}
article > div#visual{width:511px; height:132px; margin:auto; padding-top:150px;}
article > div#visual p{text-align:center;}
article > div#visual p:nth-child(1){font-size:42px;}
article > div#visual p:nth-child(3){font-size:18px;}
article > div#visual hr{width:40px; height:2px; background:#000; margin:35px auto 35px;}

article div#menu1{width:170px; height:38px; margin:138px auto; background:#fff; text-align:center; line-height:38px; border:1px solid #ccc; box-sizing:border-box;}
article div#menu1{background:yellow; font-weight:bold;}

/* 컨텐츠 */
section{width:1150px; height:922px; margin:0 auto 97px; font-family: 'Nanum Gothic';}
section > div.coffeeclass li{height:150px; margin-bottom:25px; border:1px solid #e5e5e5; box-sizing:border-box; transition:all 0.4s;}
section > div.coffeeclass li:hover{border:1px solid #0e347e;}
section div.coffeeclass li div.leftclass{width:547px; height:149px; float:left; text-indent:-9999px; }
section div.coffeeclass li:nth-child(1) div.leftclass{ background:url(../images_community/seminar_04.jpg) no-repeat center;}
section div.coffeeclass li:nth-child(2) div.leftclass{ background:url(../images_community/seminar_1.jpg) no-repeat center;}
section div.coffeeclass li:nth-child(3) div.leftclass{ background:url(../images_community/seminar_12.jpg) no-repeat center;}
section div.coffeeclass li:nth-child(4) div.leftclass{ background:url(../images_community/seminar_11.jpg) no-repeat center;}
section div.coffeeclass li:nth-child(5) div.leftclass{ background:url(../images_community/seminar_10.jpg) no-repeat center;}


section div.coffeeclass li div.rightclass{float:right; width:526px; height:149px;}
section div.coffeeclass li div.rightclass span{width:76px; height:32px; display:block; background:#333333; color:#666666; border-radius:30px; text-align:center; line-height:32px; margin-top:20px;}
section div.coffeeclass li div.rightclass h3{color:#b9b9c9; font-size:28px; margin:10px 0 10px;}
section div.coffeeclass li:hover a h3{color:#0e347e;}
section div.coffeeclass li div.rightclass p{color:#666666; font-weight:bold;}
section div.coffeeclass li:hover a h3{color:#0e347e;}


section div.number{width:220px; height:30px; margin:75px auto 0;}
section div.number li{float:left; margin-right:25px; line-height:30px; font-weight:bold; }
section div.number li:nth-child(5){margin-right:30px;}
section div.number li:last-child{margin-right:0px; border:1px solid #eeeeee; width:30px; height:30px; float:right;}
section div.number li:last-child img{padding:13px; width:5px; height:6px;}
section div.number li a{color:#caccd1}
section div.number li a.on{color:#0e347e; font-weight:bold;}
section div.number li a:hover{color:#0e347e; font-weight:bold;}

/* 푸터 */
div#footer{height:257px; border-top:3px solid #ffeb00;}
div#footer > div{width:1150px; height:254px; margin:auto;}


div#footer h3{width:179px; height:47px; margin:30px auto; text-indent:-9999px; background:url(../images/flogo.png); background-size:cover; margin-bottom:35px;}
div#footer div#address{width:516px; height:142px; margin:auto; text-align:center;}
div#footer div address{font-style:normal; line-height:22px; font-size:12px; margin-bottom:40px;}
div#footer div address span{border-right:1px solid #000; padding:0 16px;}
div#footer div address span:nth-child(3){border-right:none;}
div#footer div address span:nth-child(2)#bo{border-right:none;}
div#footer div p#copy{text-transform:uppercase; font-size:13px;}

div#footer div#family{float:right; width:175px; height:24px; border:1px solid #000; font-size:13px; line-height:24px;}
div#footer div#family a{margin-left:7px;}
div#footer h4 a{width:60px; height:60px; background:url(../images/off.png) no-repeat; position:fixed; right:200px; bottom:20px;}